{
    "manifestVersion": 1,
    "scopes": [
        "vso.code"
    ],
    "repository": {
        "type": "git",
        "uri": "https://github.com/javiramos1/azure-devops-mermaid"
    },
    "contributions": [
        {
            "id": "mermaid-renderer",
            "type": "ms.vss-code-web.content-renderer",
            "description": "Renders Mermaid diagrams in Markdown files with full GFM support",
            "targets": [
                "ms.vss-code-web.content-renderer-collection"
            ],
            "properties": {
                "uri": "dist/index.html",
                "fileExtensions": [
                    "md"
                ],
                "mimeTypes": [
                    "text/html"
                ],
                "defaultBehavior": "showRenderedContent",
                "registeredObjectId": "mermaid_renderer"
            }
        }
    ],
    "contributionTypes": []
}