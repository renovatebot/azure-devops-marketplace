{
    "manifestVersion": 1,
    "demands": [
        "api-version/3.0"
    ],
    "scopes": [
        "vso.work",
        "vso.dashboards_manage",
        "vso.test_write",
        "vso.graph",
        "vso.work_write",
        "vso.profile_write"
    ],
    "contributions": [
        {
            "id": "t8n-group",
            "type": "ms.vss-web.hub-group",
            "description": "Power Testmation",
            "targets": [
                "ms.vss-web.project-hub-groups-collection"
            ],
            "properties": {
                "name": "Power Testmation",
                "icon": {
                    "dark": "static/images/logo.png",
                    "light": "static/images/logo.png"
                }
            }
        },
        {
            "id": "panel-tplan-data",
            "type": "ms.vss-web.external-content",
            "properties": {
                "uri": "dist/PanelTPlanData/PanelTPlanData.html"
            }
        },
        {
            "id": "panel-select-items-with-query",
            "type": "ms.vss-web.external-content",
            "properties": {
                "uri": "dist/PanelSelectItemsWithQuery/PanelSelectItemsWithQuery.html"
            }
        },
        {
            "id": "panel-delete-test-container",
            "type": "ms.vss-web.external-content",
            "properties": {
                "uri": "dist/PanelDeleteTestsContainer/PanelDeleteTestsContainer.html"
            }
        },
        {
            "id": "panel-view-test-runs",
            "type": "ms.vss-web.external-content",
            "properties": {
                "uri": "dist/PanelViewTestRuns/PanelViewTestRuns.html"
            }
        },
        {
            "id": "panel-view-bugs-of-runs",
            "type": "ms.vss-web.external-content",
            "properties": {
                "uri": "dist/PanelViewBugsOfRuns/PanelViewBugsOfRuns.html"
            }
        },
        {
            "id": "testPlans",
            "type": "ms.vss-web.hub",
            "targets": [
                ".t8n-group"
            ],
            "properties": {
                "name": "Test plans",
                "iconName": "TestPlan",
                "uri": "dist/TestPlans/TestPlans.html"
            }
        },
        {
            "id": "myTestPlan",
            "type": "ms.vss-web.hub",
            "targets": [
                ".t8n-group"
            ],
            "properties": {
                "name": "Active work",
                "iconName": "TestSuite",
                "uri": "dist/TestPlan/TestPlan.html"
            }
        },
        {
            "id": "work-item-open-hub",
            "type": "ms.vss-web.hub",
            "targets": [
                "ms.vss-work-web.work-hub-group"
            ],
            "properties": {
                "name": "Sample work item open",
                "uri": "dist/WorkItemOpen/WorkItemOpen.html",
                "icon": {
                    "light": "static/add-grey.png",
                    "dark": "static/add-green.png"
                }
            }
        }
    ],
    "contributionTypes": []
}