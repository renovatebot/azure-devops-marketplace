<?xml version="1.0" encoding="utf-8"?>
<PackageManifest Version="2.0.0" xmlns="http://schemas.microsoft.com/developer/vsx-schema/2011" xmlns:d="http://schemas.microsoft.com/developer/vsx-schema-design/2011">
  <Metadata>
    <Identity Language="en-US" Id="mutation-report-publisher" Version="2.0.0" Publisher="stryker-mutator"/>
    <DisplayName>Mutation Report Publisher</DisplayName>
    <Description xml:space="preserve">With this extension you add the mutation report to your build summary so you can view the mutation test result on azure devops.</Description>
    <GalleryFlags>Public Preview</GalleryFlags>
    <Properties>
      <Property Id="Microsoft.VisualStudio.Services.Branding.Color" Value="#e84b39"/>
      <Property Id="Microsoft.VisualStudio.Services.Branding.Theme" Value="light"/>
      <Property Id="Microsoft.VisualStudio.Services.Links.GitHub" Value="https://github.com/stryker-mutator/azure-devops-mutationreport-publisher"/>
    </Properties>
    <Badges>
      <Badge Link="https://dev.azure.com/stryker-mutator/Stryker/_build/latest?definitionId=10" ImgUri="https://img.shields.io/azure-devops/build/stryker-mutator/Stryker/10/master.svg?label=Azure%20Pipelines&amp;style=flat-square" Description="Azure Pipelines Build"/>
    </Badges>
    <Tags>Build,Test,Mutation Testing,Mutation Result,Mutating,Mutation Report,Quality,Test Quality</Tags>
    <Categories>Azure Pipelines</Categories>
    <Icon>extension/icons/icon-128px.png</Icon>
    <License>LICENSE</License>
  </Metadata>
  <Dependencies/>
  <Installation>
    <InstallationTarget Id="Microsoft.VisualStudio.Services"/>
  </Installation>
  <Assets>
    <Asset Type="extension/icons/icon-128px.png" d:Source="File" Path="extension/icons/icon-128px.png" Addressable="true"/>
    <Asset Type="Microsoft.VisualStudio.Services.Icons.Default" d:Source="File" Path="extension/icons/icon-128px.png" Addressable="true"/>
    <Asset Type="extension/icons/icon-default.png" d:Source="File" Path="extension/icons/icon-default.png" Addressable="true"/>
    <Asset Type="mutation-report-tab/build-results-report-tab.html" d:Source="File" Path="mutation-report-tab/build-results-report-tab.html" Addressable="true"/>
    <Asset Type="mutation-report-tab/main.js" d:Source="File" Path="mutation-report-tab/main.js" Addressable="true"/>
    <Asset Type="mutation-report-tab/main.js.LICENSE.txt" d:Source="File" Path="mutation-report-tab/main.js.LICENSE.txt" Addressable="true"/>
    <Asset Type="Microsoft.VisualStudio.Services.Content.Details" d:Source="File" Path="docs/extension-description.md" Addressable="true"/>
    <Asset Type="Microsoft.VisualStudio.Services.Content.License" d:Source="File" Path="LICENSE" Addressable="true"/>
    <Asset Type="Microsoft.VisualStudio.Services.Manifest" d:Source="File" Path="extension.vsomanifest" Addressable="true"/>
  </Assets>
</PackageManifest>