            "targets": [
                "ms.vss-dashboards-web.widget-catalog"
            ],
            "properties": {
                "name": "Top Time-Consuming Tasks",
                "description": "Tasks ranked by total time spent",
                "catalogIconUrl": "img/widget-top-tasks.png",
                "uri": "dist/widget-top-tasks.html",
                "supportedSizes": [
                    {
                        "rowSpan": 2,
                        "columnSpan": 2
                    }
                ],
                "supportedScopes": [
                    "project_team"
                ]
            }
        },
        {
            "id": "team-productivity-widget",
            "type": "ms.vss-dashboards-web.widget",
            "description": "Team productivity trend line chart",
            "targets": [
                "ms.vss-dashboards-web.widget-catalog"
            ],
            "properties": {
                "name": "Team Productivity Trend",
                "description": "Line chart showing time per sprint over multiple sprints",
                "catalogIconUrl": "img/widget-trend.png",
                "uri": "dist/widget-productivity-trend.html",
                "supportedSizes": [
