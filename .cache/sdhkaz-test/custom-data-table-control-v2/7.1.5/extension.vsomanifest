{
    "manifestVersion": 1,
    "scopes": [
        "vso.work"
    ],
    "contributions": [
        {
            "id": "custom-data-table-control",
            "type": "ms.vss-work-web.work-item-form-control",
            "description": "Contributes a custom control that renders an editable data table.",
            "targets": [
                "ms.vss-work-web.work-item-form"
            ],
            "properties": {
                "name": "Custom Data Table",
                "uri": "dist/index.html",
                "height": 360
            },
            "inputs": [
                {
                    "id": "DataFieldRefName",
                    "name": "Data field (reference name)",
                    "description": "Work item field where the table JSON is stored (example: Custom.DataTableJson).",
                    "type": "WorkItemField",
                    "defaultValue": "Custom.DataTableJson",
                    "properties": {
                        "workItemFieldTypes": [
                            "String",
                            "PlainText",
                            "HTML"
                        ]
                    },
                    "validation": {
                        "dataType": "String",
                        "isRequired": true
                    }
                }
            ]
        }
    ],
    "contributionTypes": []
}