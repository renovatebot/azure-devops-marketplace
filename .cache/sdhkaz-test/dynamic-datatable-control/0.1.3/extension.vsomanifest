{
    "manifestVersion": 1,
    "repository": {
        "type": "git",
        "uri": "https://github.com/sdhkaz/dynamic-datatable-extension"
    },
    "scopes": [
        "vso.work"
    ],
    "contributions": [
        {
            "id": "datatable-workitem-control",
            "type": "ms.vss-work-web.work-item-form-control",
            "targets": [
                "ms.vss-work-web.work-item-form"
            ],
            "properties": {
                "name": "Dynamic DataTable",
                "uri": "dist/index.html",
                "height": 450,
                "inputs": [],
                "supportedSizes": [
                    {
                        "id": "default",
                        "name": "Default",
                        "height": 450
                    },
                    {
                        "id": "compact",
                        "name": "Compact",
                        "height": 300
                    },
                    {
                        "id": "large",
                        "name": "Large",
                        "height": 600
                    }
                ]
            }
        }
    ],
    "contributionTypes": []
}