{
    "manifestVersion": 1,
    "scopes": [
        "vso.work",
        "vso.work_write"
    ],
    "demands": [
        "api-version/3.0"
    ],
    "contributions": [
        {
            "id": "datatable-group",
            "type": "ms.vss-work-web.work-item-form-group",
            "description": "Shows a Data Table in the main Work Item form.",
            "targets": [
                "ms.vss-work-web.work-item-form"
            ],
            "properties": {
                "name": "Data Table",
                "uri": "dist/workitem-datatable.html",
                "height": 480
            }
        },
        {
            "id": "datatable-page",
            "type": "ms.vss-work-web.work-item-form-page",
            "description": "Shows a Data Table on its own tab.",
            "targets": [
                "ms.vss-work-web.work-item-form"
            ],
            "properties": {
                "name": "Data Table",
                "uri": "dist/workitem-datatable.html"
            }
        },
        {
            "id": "datatable-control",
            "type": "ms.vss-work-web.work-item-form-control",
            "description": "Data Table custom control (optional).",
            "targets": [
                "ms.vss-work-web.work-item-form"
            ],
            "properties": {
                "name": "Data Table (Control)",
                "uri": "dist/workitem-datatable.html",
                "height": 480
            }
        }
    ],
    "contributionTypes": []
}