<?xml version="1.0" encoding="utf-8"?>
<PackageManifest Version="2.0.0" xmlns="http://schemas.microsoft.com/developer/vsx-schema/2011" xmlns:d="http://schemas.microsoft.com/developer/vsx-schema-design/2011">
  <Metadata>
    <Identity Language="en-US" Id="dynamic-datatable-control" Version="5.0.0" Publisher="sdhkaz-test"/>
    <DisplayName>Work Item Datatable (Ankit)</DisplayName>
    <Description xml:space="preserve">Adds a dynamic data table to the Work Item form. Data persists per work item via Extension Data Storage.</Description>
    <GalleryFlags>Public</GalleryFlags>
    <Categories>Azure Boards</Categories>
    <Properties>
      <Property Id="Microsoft.VisualStudio.Services.Branding.Color" Value="#0e639c"/>
      <Property Id="Microsoft.VisualStudio.Services.Branding.Theme" Value="light"/>
      <Property Id="Microsoft.VisualStudio.Services.Links.Getstarted" Value="https://learn.microsoft.com/azure/devops/extend/develop/add-workitem-extension"/>
      <Property Id="Microsoft.VisualStudio.Services.Links.Support" Value="https://learn.microsoft.com/azure/devops/extend/overview"/>
    </Properties>
    <Icon>images/icon.png</Icon>
  </Metadata>
  <Dependencies/>
  <Installation>
    <InstallationTarget Id="Microsoft.VisualStudio.Services"/>
  </Installation>
  <Assets>
    <Asset Type="dist/styles.css" d:Source="File" Path="dist/styles.css" Addressable="true"/>
    <Asset Type="dist/workitem-datatable.html" d:Source="File" Path="dist/workitem-datatable.html" Addressable="true"/>
    <Asset Type="dist/workitem-datatable.js" d:Source="File" Path="dist/workitem-datatable.js" Addressable="true"/>
    <Asset Type="images/icon.png" d:Source="File" Path="images/icon.png" Addressable="true"/>
    <Asset Type="Microsoft.VisualStudio.Services.Icons.Default" d:Source="File" Path="images/icon.png" Addressable="true"/>
    <Asset Type="Microsoft.VisualStudio.Services.Content.Details" d:Source="File" Path="README.md" Addressable="true"/>
    <Asset Type="Microsoft.VisualStudio.Services.Manifest" d:Source="File" Path="extension.vsomanifest" Addressable="true"/>
  </Assets>
</PackageManifest>