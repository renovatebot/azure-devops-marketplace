{
    "manifestVersion": 1,
    "repository": {
        "type": "git",
        "uri": "https://github.com/sdhkaz/dynamic-datatable-extension"
    },
    "scopes": [
        "vso.work"
    ],
    "contributions": [
        {
            "id": "datatable-workitem-control",
            "type": "ms.vss-work-web.work-item-form-control",
            "targets": [
                "ms.vss-work-web.work-item-form"
            ],
            "properties": {
                "name": "Dynamic DataTable",
                "uri": "dist/index.html",
                "height": 500,
                "isFormContribution": true,
                "inputs": [
                    {
                        "id": "FieldName",
                        "name": "Field Name",
                        "description": "The field to store/load DataTable data",
                        "type": "WorkItemField",
                        "properties": {
                            "workItemFieldTypes": [
                                "String",
                                "PlainText",
                                "HTML"
                            ]
                        },
                        "validation": {
                            "dataType": "String",
                            "isRequired": true
                        }
                    }
                ],
                "supportedSizes": [
                    {
                        "id": "default",
                        "name": "Default",
                        "height": 500
                    },
                    {
                        "id": "compact",
                        "name": "Compact",
                        "height": 350
                    },
                    {
                        "id": "large",
                        "name": "Large",
                        "height": 700
                    }
                ]
            }
        }
    ],
    "contributionTypes": []
}