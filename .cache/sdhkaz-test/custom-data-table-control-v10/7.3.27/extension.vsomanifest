{
    "manifestVersion": 1,
    "scopes": [
        "vso.work_write",
        "vso.work"
    ],
    "contributions": [
        {
            "id": "custom-data-table-control-dt-v3",
            "type": "ms.vss-work-web.work-item-form-control",
            "targets": [
                "ms.vss-work-web.work-item-form"
            ],
            "description": "Adds a custom work item form control that shows an editable DataTables grid.",
            "properties": {
                "name": "Custom Data Table (DataTables) v3",
                "uri": "dist/index.html",
                "height": 380
            },
            "inputs": [
                {
                    "id": "SchemaFieldRefName",
                    "name": "Schema field (reference name)",
                    "type": "WorkItemField",
                    "description": "Field that stores the schema JSON",
                    "defaultValue": "Custom.DataTableSchema",
                    "properties": {
                        "workItemFieldTypes": [
                            "String",
                            "PlainText",
                            "Html"
                        ]
                    }
                },
                {
                    "id": "SchemaFieldRefNameText",
                    "name": "Schema field (manual entry)",
                    "type": "string",
                    "defaultValue": "Custom.DataTableSchema"
                },
                {
                    "id": "DataFieldRefName",
                    "name": "Data field (reference name)",
                    "type": "WorkItemField",
                    "description": "Field that stores the table rows JSON",
                    "defaultValue": "Custom.DataTableJSON",
                    "properties": {
                        "workItemFieldTypes": [
                            "String",
                            "PlainText",
                            "Html"
                        ]
                    }
                },
                {
                    "id": "DataFieldRefNameText",
                    "name": "Data field (manual entry)",
                    "type": "string",
                    "defaultValue": "Custom.DataTableJSON"
                }
            ]
        }
    ],
    "contributionTypes": []
}