{
    "manifestVersion": 1,
    "contributions": [
        {
            "id": "workitem-assistant-feature",
            "type": "ms.vss-web.feature",
            "description": "Enables the ADO Work Item Assistant extension, that leverages AI to help users create and manage high-quality work items efficiently.",
            "targets": [
                "ms.vss-web.managed-features",
                "ms.vss-web.managed-features-onprem"
            ],
            "properties": {
                "name": "ADO Work Item Assistant",
                "userConfigurable": true,
                "hostConfigurable": true,
                "hostScopes": [
                    "project",
                    null
                ],
                "defaultState": true,
                "defaultValueRules": [
                    {
                        "name": "FeatureFlag",
                        "featureFlagName": "ms-adowia-external.workitem-assistant-extension-external.workitem.assistant.feature.default",
                        "defaultValue": true
                    }
                ],
                "links": {
                    "learn": {
                        "uri": "https://aka.ms/ADOWorkItemAssistant"
                    }
                }
            }
        },
        {
            "id": "feedback-dialog-content",
            "type": "ms.vss-web.external-content",
            "properties": {
                "uri": "FeedbackDialog/FeedbackDialog.html"
            },
            "constraints": [
                {
                    "name": "Feature",
                    "properties": {
                        "featureId": "ms-adowia-external.workitem-assistant-extension-external.workitem-assistant-feature"
                    }
                }
            ]
        },
        {
            "id": "preview-changes-dialog-content",
            "type": "ms.vss-web.external-content",
            "properties": {
                "uri": "PreviewChangesDialog/PreviewChangesDialog.html"
            },
            "constraints": [
                {
                    "name": "Feature",
                    "properties": {
                        "featureId": "ms-adowia-external.workitem-assistant-extension-external.workitem-assistant-feature"
                    }
                }
            ]
        },
        {
            "id": "summarize-work-item-menu",
            "type": "ms.vss-web.action",
            "description": "Summarize work item toolbar menu item which summarizes the work item",
            "targets": [
                "ms.vss-work-web.work-item-context-menu"
            ],
            "properties": {
                "text": "Work Item Insights Assistant",
                "title": "Summarize work item and discussions",
                "toolbarText": "Work Item Insights Assistant",
                "icon": "static/images/ado_wia.png",
                "uri": "SummarizeWorkItemMenu/SummarizeWorkItemMenu.html"
            },
            "constraints": [
                {
                    "name": "Feature",
                    "properties": {
                        "featureId": "ms-adowia-external.workitem-assistant-extension-external.workitem-assistant-feature"
                    }
                }
            ]
        },
        {
            "id": "summarize-workitem-panel-content",
            "type": "ms.vss-web.external-content",
            "properties": {
                "uri": "SummarizeWorkItemPanel/SummarizeWorkItemPanel.html"
            },
            "constraints": [
                {
                    "name": "Feature",
                    "properties": {
                        "featureId": "ms-adowia-external.workitem-assistant-extension-external.workitem-assistant-feature"
                    }
                }
            ]
        },
        {
            "id": "work-item-assistant-hub",
            "type": "ms.vss-web.hub",
            "description": "Work item creating and editing assistant in Azure Boards.",
            "targets": [
                "ms.vss-work-web.work-hub-group"
            ],
            "properties": {
                "name": "AI work item generator",
                "uri": "WorkItemCopilot/index.html",
                "icon": {
                    "light": "static/logos/logo_light.png",
                    "dark": "static/logos/logo_dark.png"
                }
            },
            "constraints": [
                {
                    "name": "Feature",
                    "properties": {
                        "featureId": "ms-adowia-external.workitem-assistant-extension-external.workitem-assistant-feature"
                    }
                }
            ]
        },
        {
            "id": "work-item-assistant-form-group",
            "type": "ms.vss-work-web.work-item-form-group",
            "description": "An Azure DevOps extension for work item assistant form group",
            "targets": [
                "ms.vss-work-web.work-item-form"
            ],
            "properties": {
                "name": "Work Item Assistant",
                "uri": "WorkItemCopilotFormGroup/index.html",
                "height": 600
            },
            "constraints": [
                {
                    "name": "Feature",
                    "properties": {
                        "featureId": "ms-adowia-external.workitem-assistant-extension-external.workitem-assistant-feature"
                    }
                }
            ]
        },
        {
            "id": "child-item-generator-assistant-form-page",
            "type": "ms.vss-work-web.work-item-form-page",
            "description": "An Azure DevOps extension for child work item generator form page",
            "targets": [
                "ms.vss-work-web.work-item-form"
            ],
            "properties": {
                "name": "Child Item Generator",
                "uri": "WorkItemCopilotFormPage/index.html"
            },
            "constraints": [
                {
                    "name": "Feature",
                    "properties": {
                        "featureId": "ms-adowia-external.workitem-assistant-extension-external.workitem-assistant-feature"
                    }
                }
            ]
        },
        {
            "id": "work-item-assistant-org-settings-admin-hub",
            "type": "ms.vss-web.hub",
            "description": "Work item creating and editing assistant collection/account level admin settings hub",
            "targets": [
                "ms.vss-web.collection-admin-hub-group"
            ],
            "properties": {
                "name": "Work Item Assistant",
                "uri": "WorkItemCopilotOrgSettings/index.html",
                "order": 100
            },
            "constraints": [
                {
                    "name": "Feature",
                    "properties": {
                        "featureId": "ms-adowia-external.workitem-assistant-extension-external.workitem-assistant-feature"
                    }
                }
            ]
        },
        {
            "id": "work-item-assistant-project-settings-admin-hub",
            "type": "ms.vss-web.hub",
            "description": "Work item creating and editing assistant project-level admin settings hub",
            "targets": [
                "ms.vss-web.project-admin-hub-group"
            ],
            "properties": {
                "name": "Work Item Assistant",
                "uri": "WorkItemCopilotProjectSettings/index.html",
                "order": 100
            },
            "constraints": [
                {
                    "name": "Feature",
                    "properties": {
                        "featureId": "ms-adowia-external.workitem-assistant-extension-external.workitem-assistant-feature"
                    }
                }
            ]
        }
    ],
    "scopes": [
        "vso.project",
        "vso.work",
        "vso.work_write",
        "vso.graph",
        "vso.settings"
    ],
    "baseUri": "https://ado-wia-agent-appservice-uat-bkfkg5d7bra5ejcu.westus2-01.azurewebsites.net",
    "contributionTypes": []
}