      <Property Id="Microsoft.VisualStudio.Services.Links.GitHub" Value="https://github.com/kiuwan/tfs-vsts-extension"/>
      <Property Id="Microsoft.VisualStudio.Services.Branding.Color" Value="#01a4a6"/>
      <Property Id="Microsoft.VisualStudio.Services.Branding.Theme" Value="dark"/>
    </Properties>
    <Tags>SAST,Code Analysis,DevOps,DevSecOps,Applcation security,Security vulnerabilities,OWASP Top 10,Build Task,SCA,Security,Quality</Tags>
    <Icon>images/kiuwan-task.png</Icon>
