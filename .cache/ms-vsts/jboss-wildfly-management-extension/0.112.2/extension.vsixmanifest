    <Asset Type="images/AddNewConnection.png" d:Source="File" Path="images/AddNewConnection.png" Addressable="true"/>
    <Asset Type="images/WildFlyConnection.png" d:Source="File" Path="images/WildFlyConnection.png" Addressable="true"/>
    <Asset Type="images/deploymenttask.png" d:Source="File" Path="images/deploymenttask.png" Addressable="true"/>
    <Asset Type="images/managementtask.png" d:Source="File" Path="images/managementtask.png" Addressable="true"/>
