<?xml version="1.0" encoding="utf-8"?>
<PackageManifest Version="2.0.0" xmlns="http://schemas.microsoft.com/developer/vsx-schema/2011" xmlns:d="http://schemas.microsoft.com/developer/vsx-schema-design/2011">
  <Metadata>
    <Identity Language="en-US" Id="services-flowdock" Version="1.0.3" Publisher="ms-vsts"/>
    <DisplayName>Flowdock Integration</DisplayName>
    <Description xml:space="preserve">Chat and inbox for Visual Studio Team Services teams. One place to talk and stay up-to-date. Flowdock is a team collaboration app for desktop, mobile and web.</Description>
    <GalleryFlags>Public</GalleryFlags>
    <Categories>Collaborate</Categories>
    <Tags>chat,rally,teamroom</Tags>
    <Properties>
      <Property Id="Microsoft.VisualStudio.Services.Links.Getstarted" Value="https://www.visualstudio.com/docs/service-hooks/services/flowdock"/>
      <Property Id="Microsoft.VisualStudio.Services.Links.Support" Value="https://www.visualstudio.com/team-services/support/"/>
      <Property Id="Microsoft.VisualStudio.Services.Links.Pricing" Value="https://www.flowdock.com/pricing"/>
      <Property Id="Microsoft.VisualStudio.Services.Links.Twitter" Value="https://twitter.com/flowdock"/>
      <Property Id="Microsoft.VisualStudio.Services.Branding.Color" Value="#3f4447"/>
      <Property Id="Microsoft.VisualStudio.Services.Branding.Theme" Value="dark"/>
    </Properties>
    <Icon>icons/logo2.png</Icon>
  </Metadata>
  <Dependencies/>
  <Installation>
    <InstallationTarget Id="Microsoft.VisualStudio.Services.Integration"/>
  </Installation>
  <Assets>
    <Asset Type="Microsoft.VisualStudio.Services.Content.Details" d:Source="File" Path="details.md" Addressable="true"/>
    <Asset Type="Microsoft.VisualStudio.Services.Icons.Default" d:Source="File" Path="icons/logo2.png" Addressable="true"/>
    <Asset Type="Microsoft.VisualStudio.Services.Icons.Large" d:Source="File" Path="icons/logo2-large.png" Addressable="true"/>
    <Asset Type="screenshots/demo-screenshots.jpg" d:Source="File" Path="screenshots/demo-screenshots.jpg" Addressable="true"/>
    <Asset Type="screenshots/notification.png" d:Source="File" Path="screenshots/notification.png" Addressable="true"/>
  </Assets>
</PackageManifest>