{
    "manifestVersion": 1,
    "contributions": [
        {
            "id": "requirement-evaluation-hub",
            "type": "ms.vss-web.hub",
            "description": "CMMI Requirements Quality Evaluation Hub",
            "targets": [
                "ms.vss-work-web.work-hub-group"
            ],
            "properties": {
                "name": "Requirements Quality",
                "uri": "dist/index.html",
                "icon": "images/logo.png",
                "order": 100
            }
        },
        {
            "id": "requirement-evaluation-panel",
            "type": "ms.vss-work-web.work-item-form-page",
            "description": "AI Readiness & Compliance panel for Requirements",
            "targets": [
                "ms.vss-work-web.work-item-form"
            ],
            "properties": {
                "name": "AI Readiness & Compliance",
                "uri": "dist/panel.html",
                "height": 600
            }
        }
    ],
    "scopes": [
        "vso.work",
        "vso.work_write"
    ],
    "CustomerQnASupport": {
        "enableqna": "true",
        "url": "https://github.com/your-org/cmmi-requirements-agent/issues"
    },
    "baseUri": "https://{{PublisherName}}.gallery.vsassets.io/{{ExtensionName}}/{{Version}}",
    "contributionTypes": []
}