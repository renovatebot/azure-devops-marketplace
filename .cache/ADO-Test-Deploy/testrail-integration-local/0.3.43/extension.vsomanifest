{
    "manifestVersion": 1,
    "scopes": [
        "vso.work",
        "vso.extension.data_write"
    ],
    "contributions": [
        {
            "id": "testrail-cases-group",
            "type": "ms.vss-work-web.work-item-form-group",
            "description": "Displays TestRail test cases linked to the current work item.",
            "targets": [
                "ms.vss-work-web.work-item-form"
            ],
            "properties": {
                "name": "TestRail: Cases",
                "uri": "dist/panels/cases.html",
                "height": 300
            }
        },
        {
            "id": "testrail-runs-group",
            "type": "ms.vss-work-web.work-item-form-group",
            "description": "Displays TestRail test runs linked to the current work item.",
            "targets": [
                "ms.vss-work-web.work-item-form"
            ],
            "properties": {
                "name": "TestRail: Runs",
                "uri": "dist/panels/runs.html",
                "height": 300
            }
        },
        {
            "id": "testrail-results-group",
            "type": "ms.vss-work-web.work-item-form-group",
            "description": "Displays TestRail test results associated with the current work item.",
            "targets": [
                "ms.vss-work-web.work-item-form"
            ],
            "properties": {
                "name": "TestRail: Results",
                "uri": "dist/panels/results.html",
                "height": 400
            }
        },
        {
            "id": "testrail-config-hub",
            "type": "ms.vss-web.hub",
            "description": "Configuration page for TestRail connection settings.",
            "targets": [
                "ms.vss-web.project-admin-hub-group"
            ],
            "properties": {
                "name": "TestRail",
                "order": 100,
                "uri": "dist/config/config.html"
            }
        }
    ],
    "contributionTypes": []
}