{
    "manifestVersion": 1,
    "repository": {
        "type": "git",
        "uri": "https://github.com/testrail/ado-integration"
    },
    "badges": [
        {
            "href": "https://marketplace.visualstudio.com/items?itemName=testrail-official.testrail-integration",
            "uri": "https://img.shields.io/badge/marketplace-testrail--integration-blue",
            "description": "Azure DevOps Marketplace"
        }
    ],
    "scopes": [
        "vso.work",
        "vso.project",
        "vso.identity"
    ],
    "demands": [],
    "contributions": [
        {
            "id": "testrail-hub",
            "type": "ms.vss-web.hub",
            "description": "TestRail Integration Hub",
            "targets": [
                "ms.vss-web.project-admin-hub-group"
            ],
            "properties": {
                "name": "TestRail Integration",
                "order": 100,
                "uri": "dist/hub.html",
                "supportsMobile": false
            }
        },
        {
            "id": "testrail-configuration-hub",
            "type": "ms.vss-web.hub",
            "description": "TestRail Configuration",
            "targets": [
                "ms.vss-web.project-admin-hub-group"
            ],
            "properties": {
                "name": "TestRail Configuration",
                "order": 101,
                "uri": "dist/configuration.html"
            }
        },
        {
            "id": "linked-test-cases-panel",
            "type": "ms.vss-work-web.work-item-form-group",
            "description": "Displays linked TestRail test cases for Azure DevOps work items",
            "targets": [
                "ms.vss-work-web.work-item-form"
            ],
            "properties": {
                "name": "Linked Test Cases",
                "uri": "dist/work-item-form.html",
                "height": 400
            }
        }
    ],
    "contributionTypes": [],
    "CustomerQnASupport": {
        "enableqna": true,
        "url": "https://support.testrail-ado-integration.com/qna"
    }
}