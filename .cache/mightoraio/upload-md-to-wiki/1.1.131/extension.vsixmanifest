      <Property Id="Microsoft.VisualStudio.Services.Links.Learn" Value="https://mightora.io"/>
      <Property Id="Microsoft.VisualStudio.Services.Links.Support" Value="https://mightora.io"/>
      <Property Id="Microsoft.VisualStudio.Services.Links.Repository" Value="https://github.com/mightora/DevOpsExtension-Upload-MD-To-Wiki/"/>
      <Property Id="Microsoft.VisualStudio.Services.Links.Issues" Value="https://github.com/mightora/DevOpsExtension-Upload-MD-To-Wiki/issues"/>
      <Property Id="Microsoft.VisualStudio.Services.Links.GitHub" Value="https://github.com/mightora/DevOpsExtension-Upload-MD-To-Wiki"/>
    </Properties>
    <Icon>images/mightora_io_icon_260x260.png</Icon>
    <License>LICENSE</License>
  </Metadata>
  <Dependencies/>
