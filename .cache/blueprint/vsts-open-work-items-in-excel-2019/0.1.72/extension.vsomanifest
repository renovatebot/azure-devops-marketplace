{
    "manifestVersion": 1,
    "scopes": [
        "vso.work",
        "vso.work_write"
    ],
    "contributions": [
        {
            "id": "openQueryAction",
            "type": "ms.vss-web.action",
            "description": "Open in Excel context menu on a query definition.",
            "targets": [
                "ms.vss-work-web.work-item-query-menu"
            ],
            "properties": {
                "text": "Open in Excel",
                "icon": "img/miniexcellogo.png",
                "group": "actions",
                "uri": "index.html"
            }
        },
        {
            "id": "openWorkItemsAction",
            "type": "ms.vss-web.action",
            "description": "Open in Excel context menu on a work item.",
            "targets": [
                "ms.vss-work-web.work-item-context-menu"
            ],
            "properties": {
                "text": "Open in Excel",
                "icon": "img/miniexcellogo.png",
                "group": "actions",
                "uri": "index.html"
            }
        },
        {
            "id": "openQueryOnToolbarAction",
            "type": "ms.vss-web.action",
            "description": "Open in Excel toolbar menu item on the query results.",
            "targets": [
                "ms.vss-work-web.work-item-query-results-toolbar-menu"
            ],
            "properties": {
                "text": "Open in Excel",
                "icon": "img/miniexcellogo.png",
                "group": "actions",
                "uri": "index.html"
            }
        },
        {
            "id": "notificationDialog",
            "type": "ms.vss-web.control",
            "targets": [],
            "properties": {
                "uri": "dialog.html"
            }
        }
    ],
    "contributionTypes": []
}