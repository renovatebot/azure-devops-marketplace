{
    "manifestVersion": 1,
    "repository": {
        "type": "git",
        "uri": "https://github.com/jariyahfoundation/azure-devops-copilot-extension"
    },
    "badges": [
        {
            "href": "https://github.com/jariyahfoundation/azure-devops-copilot-extension",
            "uri": "https://img.shields.io/badge/GitHub-Repository-blue",
            "description": "View source code on GitHub"
        }
    ],
    "contributions": [
        {
            "id": "copilot-cli-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "task"
            }
        }
    ],
    "scopes": [
        "vso.build_execute",
        "vso.release_execute"
    ],
    "contributionTypes": []
}