{
    "manifestVersion": 1,
    "baseUrl": "https://b4ad-dev.whitesourcesoftware.com/b4ad/azure-dev-ops/v1.0",
    "CustomerQnASupport": {
        "enableMarketplaceQnA": false
    },
    "scopes": [
        "vso.build"
    ],
    "contributions": [
        {
            "id": "bolt.wss",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "whitesource-task"
            }
        },
        {
            "id": "build-tab.wss",
            "type": "ms.vss-build-web.build-results-tab",
            "description": "A tab contributing to build results view",
            "targets": [
                "ms.vss-build-web.build-results-view"
            ],
            "properties": {
                "name": "Mend Bolt",
                "uri": "whitesource/build/index.html",
                "order": 100
            }
        },
        {
            "id": "organization-settings.wss",
            "type": "ms.vss-web.hub",
            "description": "Mend organization settings",
            "targets": [
                "ms.vss-web.collection-admin-hub-group"
            ],
            "properties": {
                "name": "Mend",
                "uri": "whitesource/build/index.html",
                "order": 100
            }
        }
    ],
    "eventCallbacks": {
        "postUninstall": {
            "uri": "https://b4ad-prod.whitesourcesoftware.com/b4ad/azure-dev-ops/v1.0/webhook/"
        },
        "postInstall": {
            "uri": "https://b4ad-prod.whitesourcesoftware.com/b4ad/azure-dev-ops/v1.0/webhook/"
        }
    },
    "contributionTypes": []
}