    "contributions": [
        {
            "id": "refinement-hub",
            "type": "ms.vss-web.hub",
            "targets": [
                "ms.vss-work-web.work-hub-group"
            ],
            "properties": {
                "name": "Devops Refinement Tool",
                "uri": "dist/index.html",
                "icon": {
                    "light": "marketplace/icons/logo.png",
                    "dark": "marketplace/icons/logo.png"
                }
            }
        },
        {
            "id": "refinement-work-items-option",
            "type": "ms.vss-web.action",
            "targets": [
                "ms.vss-work-web.backlog-item-menu",
                "ms.vss-work-web.query-result-work-item-menu"
            ],
            "properties": {
                "text": "Refine work item(s)",
                "title": "Start refinement session",
                "group": "actions",
                "uri": "dist/index.html?context",
                "registeredObjectId": "refinement-work-items-option",
                "icon": "marketplace/icons/logo.png"
            }
        }
    ],
    "contributionTypes": []
