                "vso.project_read",
                "vso.work"
            ],
            "properties": {
                "name": "Task Pilot",
                "uri": "build/index.html",
                "icon": "build/logo.png"
            }
        }
    ],
    "scopes": [],
    "contributionTypes": []
}