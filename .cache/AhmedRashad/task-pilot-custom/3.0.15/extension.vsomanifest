{
    "manifestVersion": 1,
    "repository": {
        "type": "git",
        "uri": "https://github.com/your-repo/task-pilot"
    },
    "contributions": [
        {
            "id": "task-pilot-hub",
            "type": "ms.vss-web.hub",
