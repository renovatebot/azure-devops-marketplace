<?xml version="1.0" encoding="utf-8"?>
<PackageManifest Version="2.0.0" xmlns="http://schemas.microsoft.com/developer/vsx-schema/2011" xmlns:d="http://schemas.microsoft.com/developer/vsx-schema-design/2011">
  <Metadata>
    <Identity Language="en-US" Id="ReleaseNotesGenerator" Version="2.0.0" Publisher="IeuanWalker"/>
    <DisplayName>Release notes generator</DisplayName>
    <GalleryFlags>Public</GalleryFlags>
    <Properties>
      <Property Id="Microsoft.VisualStudio.Services.CustomerQnALink" Value="https://github.com/IeuanWalker/AzureDevops-GenerateReleaseNotes/issues"/>
      <Property Id="Microsoft.VisualStudio.Services.Branding.Color" Value="#3d9ae2"/>
      <Property Id="Microsoft.VisualStudio.Services.Branding.Theme" Value="dark"/>
      <Property Id="Microsoft.VisualStudio.Services.Links.Home" Value="https://github.com/IeuanWalker/AzureDevops-GenerateReleaseNotes"/>
      <Property Id="Microsoft.VisualStudio.Services.Links.Getstarted" Value="https://github.com/IeuanWalker/AzureDevops-GenerateReleaseNotes"/>
      <Property Id="Microsoft.VisualStudio.Services.Links.Issues" Value="https://github.com/IeuanWalker/AzureDevops-GenerateReleaseNotes/issues"/>
      <Property Id="Microsoft.VisualStudio.Services.Links.GitHub" Value="https://github.com/IeuanWalker/AzureDevops-GenerateReleaseNotes"/>
    </Properties>
    <Description xml:space="preserve">Generate release notes from a range of commits in your repository.</Description>
    <Categories>Azure Pipelines</Categories>
    <Tags>Release,Package,Publish</Tags>
    <Icon>images/logo.png</Icon>
  </Metadata>
  <Dependencies/>
  <Installation>
    <InstallationTarget Id="Microsoft.VisualStudio.Services"/>
  </Installation>
  <Assets>
    <Asset Type="images/logo.png" d:Source="File" Path="images/logo.png" Addressable="true"/>
    <Asset Type="Microsoft.VisualStudio.Services.Icons.Default" d:Source="File" Path="images/logo.png" Addressable="true"/>
    <Asset Type="images/oldLogo.png" d:Source="File" Path="images/oldLogo.png" Addressable="true"/>
    <Asset Type="Microsoft.VisualStudio.Services.Content.Details" d:Source="File" Path="README.md" Addressable="true"/>
    <Asset Type="Microsoft.VisualStudio.Services.Manifest" d:Source="File" Path="extension.vsomanifest" Addressable="true"/>
  </Assets>
</PackageManifest>