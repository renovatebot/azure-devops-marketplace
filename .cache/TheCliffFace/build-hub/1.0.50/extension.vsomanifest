{
    "manifestVersion": 1,
    "repository": {
        "type": "git",
        "uri": "https://github.com/TheCliffFace/azure-devops-build-hub"
    },
    "scopes": [
        "vso.build",
        "vso.build_execute"
    ],
    "contributions": [
        {
            "id": "custom-build-release-hub-group",
            "type": "ms.vss-web.hub",
            "targets": [
                "ms.vss-build-web.build-release-hub-group"
            ],
            "properties": {
                "name": "Pipelines Hub",
                "uri": "dist/build-release-hub-group/build-release-hub-group.html",
                "icon": {
                    "light": "static/app_folder_light.png",
                    "dark": "static/app_folder.png"
                }
            }
        }
    ],
    "contributionTypes": []
}