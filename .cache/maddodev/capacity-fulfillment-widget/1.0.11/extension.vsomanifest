{
    "manifestVersion": 1,
    "scopes": [
        "vso.work",
        "vso.work_full",
        "vso.project"
    ],
    "contributions": [
        {
            "id": "maddodev.capacity-fulfillment-widget.capacity-fulfillment-widget-contribution",
            "type": "ms.vss-dashboards.widget",
            "description": "Widget to show monthly capacity fulfillment.",
            "targets": [
                "ms.vss-dashboards.widget-catalog"
            ],
            "properties": {
                "name": "Monthly Capacity Fulfillment",
                "description": "Displays the team's planned capacity, fulfilled capacity, and percentage fulfillment for the current month.",
                "uri": "widget.html",
                "supportedSizes": [
                    {
                        "rowspan": 2,
                        "colspan": 2
                    }
                ],
                "supportedContexts": [
                    "project_team"
                ],
                "configuration": {
                    "uri": "widget.html",
                    "canSave": true,
                    "isEnabled": true
                }
            }
        }
    ],
    "contributionTypes": []
}