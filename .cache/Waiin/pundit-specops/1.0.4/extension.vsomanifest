    "manifestVersion": 1,
    "license": "Apache-2.0",
    "author": {
        "name": "Wai Technologies",
        "email": "support@waiin.com"
    },
    "demands": [
        "api-version/3.0"
    ],
    "scopes": [
        "vso.build_execute",
        "vso.code_write",
        "vso.work_write",
        "vso.project",
        "vso.wiki",
        "vso.packaging",
        "vso.test"
    ],
    "contributions": [
        {
            "id": "speckit-project-tab",
            "type": "ms.vss-web.hub",
            "targets": [
                "ms.vss-web.project-hub-group"
            ],
            "properties": {
                "name": "AI Chat",
                "uri": "dist/index.html",
                "supportsMobile": true,
                "order": 100
            }
        },
        {
            "id": "speckit-hub-group",
            "type": "ms.vss-web.hub-group",
            "targets": [
                "ms.vss-web.project-hub-groups-collection"
            ],
            "properties": {
                "name": "AI Pundit Spec Kit",
                "icon": "utils/assets/AIPunditLogo.png",
                "order": 100
            }
        },
        {
            "id": "speckit-hub",
            "type": "ms.vss-web.hub",
            "targets": [
                ".speckit-hub-group"
            ],
            "properties": {
                "name": "AI Chat",
                "uri": "dist/index.html",
                "order": 100
            }
        },
        {
            "id": "board-data-provider",
            "type": "ms.vss-web.data-provider",
            "targets": [
                "ms.vss-work-web.work-item-form-page-data-provider"
            ],
            "properties": {
                "name": "Board Data Provider",
                "uri": "dist/index.html"
            }
        }
    ],
