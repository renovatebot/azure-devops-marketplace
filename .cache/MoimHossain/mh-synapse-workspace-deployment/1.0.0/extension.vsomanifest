                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "synapse-deploy"
            }
        },
        {
            "id": "mh-toggle-trigger",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "toggle-trigger"
            }
        }
    ],
    "contributionTypes": []
}