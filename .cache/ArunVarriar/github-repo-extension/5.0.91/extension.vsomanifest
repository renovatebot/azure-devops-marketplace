{
    "manifestVersion": 1,
    "scopes": [
        "vso.extension.data",
        "vso.release",
        "vso.code"
    ],
    "constraints": [
        {
            "id": "http",
            "name": "GitHub API Access",
            "properties": {
                "allowedDomains": [
                    "api.github.com"
                ]
            }
        }
    ],
    "contributions": [
        {
            "id": "github-repo-hub",
            "type": "ms.vss-web.hub",
            "targets": [
                "ms.vss-web.collection-admin-hub-group"
            ],
            "properties": {
                "name": "GitHub Repo Creator",
                "uri": "src/github-data.html",
                "icon": "bowtie-icon bowtie-brand-github"
            }
        },
        {
            "id": "create-github-repo-hub",
            "type": "ms.vss-web.hub",
            "targets": [
                "ms.vss-code-web.code-hub-group"
            ],
            "properties": {
                "name": "Create GitHub Repo",
                "uri": "src/create-github-repo.html",
                "icon": "bowtie-icon bowtie-folder-fill",
                "order": 100
            }
        },
        {
            "id": "github-repo-service",
            "type": "ms.vss-web.service",
            "targets": [
                "ms.vss-web.data-service"
            ],
            "properties": {
                "uri": "scripts/github-data-service.js"
            }
        }
    ],
    "contributionTypes": [
        {
            "id": "github-repo-provider",
            "name": "GitHub Repo Creator Provider",
            "description": "Provides data storage for GitHub repository information"
        }
    ],
    "repository": {
        "type": "git",
        "uri": "https://github.com/arunvarriar/github-repo-extension"
    }
}