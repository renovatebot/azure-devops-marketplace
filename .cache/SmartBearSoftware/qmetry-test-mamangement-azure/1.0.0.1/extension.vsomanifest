{
    "manifestVersion": 1,
    "scopes": [
        "vso.work",
        "vso.project_manage",
        "vso.graph_manage"
    ],
    "contributions": [
        {
            "id": "work-item-form-page",
            "type": "ms.vss-work-web.work-item-form-page",
            "description": "Custom work item form page",
            "targets": [
                "ms.vss-work-web.work-item-form"
            ],
            "properties": {
                "name": "QMetry Test Coverage",
                "uri": "index.html#/work-item-page",
                "visible": false
            }
        },
        {
            "id": "custom-project-settings-hub",
            "type": "ms.vss-web.hub",
            "description": "QMetry settings page in Project Settings.",
            "targets": [
                "ms.vss-web.project-admin-hub-group"
            ],
            "properties": {
                "name": "QMetry Project Settings",
                "uri": "index.html#/project-settings",
                "order": 1
            }
        },
        {
            "id": "custom-org-settings-hub",
            "type": "ms.vss-web.hub",
            "description": "QMetry settings page in Organization Settings.",
            "targets": [
                "ms.vss-web.collection-admin-hub-group"
            ],
            "properties": {
                "name": "QMetry Test Management",
                "uri": "index.html#/org-settings",
                "order": 1
            }
        }
    ],
    "contributionTypes": []
}