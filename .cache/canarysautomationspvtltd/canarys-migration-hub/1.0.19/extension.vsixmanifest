      <Property Id="Microsoft.VisualStudio.Services.Links.Getstarted" Value="https://canarysmigrationhub.ecanarys.com/"/>
      <Property Id="Microsoft.VisualStudio.Services.Links.Support" Value="mailto:devops@canarys.com"/>
      <Property Id="Microsoft.VisualStudio.Services.Branding.Color" Value="#222222"/>
      <Property Id="Microsoft.VisualStudio.Services.Branding.Theme" Value="dark"/>
    </Properties>
    <Icon>images/logo.png</Icon>
  </Metadata>
  <Dependencies/>
  <Installation>
    <InstallationTarget Id="Microsoft.VisualStudio.Services.Integration"/>
  </Installation>
  <Assets>
    <Asset Type="images/cmh-logocanarys.png" d:Source="File" Path="images/cmh-logocanarys.png" Addressable="true"/>
    <Asset Type="images/logo.png" d:Source="File" Path="images/logo.png" Addressable="true"/>
    <Asset Type="Microsoft.VisualStudio.Services.Icons.Default" d:Source="File" Path="images/logo.png" Addressable="true"/>
    <Asset Type="overview.md" d:Source="File" Path="overview.md" Addressable="true"/>
    <Asset Type="Microsoft.VisualStudio.Services.Content.Details" d:Source="File" Path="overview.md" Addressable="true"/>
    <Asset Type="Microsoft.VisualStudio.Services.Links.Getstarted" d:Source="File" Path="getstarted.html" Addressable="true"/>
  </Assets>
