<?xml version="1.0" encoding="utf-8"?>
<PackageManifest Version="2.0.0" xmlns="http://schemas.microsoft.com/developer/vsx-schema/2011" xmlns:d="http://schemas.microsoft.com/developer/vsx-schema-design/2011">
  <Metadata>
    <Identity Language="en-US" Id="aoai-code-reviewer" Version="1.3.0" Publisher="Ruben-Nintex"/>
    <DisplayName>Azure OpenAI Code Reviewer</DisplayName>
    <Description xml:space="preserve">GitHub Copilot-style code review for Azure DevOps using Azure OpenAI</Description>
    <GalleryFlags>Public</GalleryFlags>
    <Categories>Azure Pipelines</Categories>
    <Tags>Code Review,AI,OpenAI,Pull Request,Git Diff,Copilot</Tags>
    <Properties>
      <Property Id="Microsoft.VisualStudio.Services.Links.GitHub" Value="https://github.com/Ruben-Nintex/aoai-review"/>
    </Properties>
    <Icon>images/icon.png</Icon>
  </Metadata>
  <Dependencies/>
  <Installation>
    <InstallationTarget Id="Microsoft.VisualStudio.Services"/>
    <InstallationTarget Id="Microsoft.TeamFoundation.Server" Version="[15.2,)"/>
  </Installation>
  <Assets>
    <Asset Type="tasks/AOAICodeReview/index.js" d:Source="File" Path="tasks/AOAICodeReview/index.js" Addressable="true"/>
    <Asset Type="tasks/AOAICodeReview/task.json" d:Source="File" Path="tasks/AOAICodeReview/task.json" Addressable="true"/>
    <Asset Type="images/icon.png" d:Source="File" Path="images/icon.png" Addressable="true"/>
    <Asset Type="Microsoft.VisualStudio.Services.Icons.Default" d:Source="File" Path="images/icon.png" Addressable="true"/>
    <Asset Type="overview.md" d:Source="File" Path="overview.md" Addressable="true"/>
    <Asset Type="Microsoft.VisualStudio.Services.Content.Details" d:Source="File" Path="overview.md" Addressable="true"/>
    <Asset Type="Microsoft.VisualStudio.Services.Manifest" d:Source="File" Path="extension.vsomanifest" Addressable="true"/>
  </Assets>
</PackageManifest>