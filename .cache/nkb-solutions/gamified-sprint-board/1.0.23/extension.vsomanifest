{
    "manifestVersion": 1,
    "publisherDisplayName": "NKB Solutions",
    "contributions": [
        {
            "id": "leaderboard-hub",
            "type": "ms.vss-web.hub",
            "description": "Leaderboard for team XP and achievements",
            "targets": [
                "ms.vss-work-web.work-hub-group"
            ],
            "properties": {
                "name": "Leaderboard",
                "uri": "dist/leaderboard.html",
                "icon": {
                    "light": "css://fabric-icon ms-Icon--VerifiedBrand",
                    "dark": "css://fabric-icon ms-Icon--VerifiedBrand"
                },
                "order": 99
            }
        },
        {
            "id": "achievements-hub",
            "type": "ms.vss-web.hub",
            "description": "Achievements overview",
            "targets": [
                "ms.vss-work-web.work-hub-group"
            ],
            "properties": {
                "name": "Achievements",
                "uri": "dist/achievements.html",
                "icon": {
                    "light": "css://fabric-icon ms-Icon--VerifiedBrand",
                    "dark": "css://fabric-icon ms-Icon--VerifiedBrand"
                },
                "order": 100
            }
        },
        {
            "id": "profile-panel",
            "type": "ms.vss-web.panel",
            "description": "User profile with XP and badges",
            "targets": [
                "."
            ],
            "properties": {
                "name": "Gamify Profile",
                "uri": "dist/profile.html"
            }
        },
        {
            "id": "admin-hub",
            "type": "ms.vss-web.hub",
            "description": "Admin panel for XP and achievement configuration",
            "targets": [
                "ms.vss-web.project-admin-hub-group"
            ],
            "properties": {
                "name": "Gamify Settings",
                "uri": "dist/admin.html",
                "icon": {
                    "light": "css://fabric-icon ms-Icon--Settings",
                    "dark": "css://fabric-icon ms-Icon--Settings"
                }
            }
        }
    ],
    "scopes": [
        "vso.work",
        "vso.project",
        "vso.identity",
        "vso.code",
        "vso.hooks_write"
    ],
    "contributionTypes": []
}